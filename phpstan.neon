includes:
	- phpstan-baseline.neon

parameters:
	checkMissingIterableValueType: false
	ergebnis:
		classesAllowedToBeExtended:
			- InvalidArgumentException
			- RuntimeException
	inferPrivatePropertyTypeFromConstructor: true
	level: max
	paths:
		- src
		- test
	tmpDir: %currentWorkingDirectory%/.build/phpstan
